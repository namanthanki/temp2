<!-- views/degrees/index.ejs -->
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Degrees</title>
</head>

<body>
  <h1>Degrees</h1>
  <ul>
    <% degrees.forEach(degree => { %>
      <li><%= degree.name %></li>
    <% }); %>
  </ul>
  <a href="/degrees/new">Add a new degree</a>
</body>

</html>